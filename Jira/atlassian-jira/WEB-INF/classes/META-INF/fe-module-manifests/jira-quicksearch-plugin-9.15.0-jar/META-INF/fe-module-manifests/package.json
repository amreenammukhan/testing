{
  "name": "jira-quicksearch-plugin",
  "description": "The frontend component pieces and build tools for the quicksearch plugin.",
  "private": true,
  "engines": {
    "node": "^14.18",
    "npm": "^6.14",
    "yarn": "^1.22"
  },
  "//": [
    "The build script will be invoked by maven in the process-resources phase",
    "via the frontend-maven-plugin. It goes through Yarn rather than",
    "calling Gulp directly, because the yarn call will work across environments"
  ],
  "scripts": {
    "build-dev": "gulp build",
    "build": "cross-env NODE_ENV=production gulp build",
    "build:report": "cross-env NODE_ENV=production BUNDLE_REPORT=true gulp build",
    "watch-prod": "cross-env NODE_ENV=production gulp watch",
    "watch": "gulp watch",
    "dev": "webpack-dev-server --inline --progress --config conf/webpack.dev.config.js",
    "test": "jest --config ./jest.config.js",
    "test:cov": "jest --config ./jest.config.js --coverage",
    "lint": "cross-env NODE_ENV=production eslint --ext .js,.vue src/main/frontend",
    "lint-dev": "eslint --ext .js,.vue src/main/frontend",
    "lint-fix": "yarn run lint --fix"
  },
  "dependencies": {
    "@atlaskit/util-shared-styles": "^2.10.8",
    "@atlassian/aui": "~9.4.10",
    "vue": "^2.5.22",
    "vuex": "^3.0.1"
  },
  "resolutions": {
    "chokidar": "^3.5.3",
    "semver": "^7.5.2"
  },
  "devDependencies": {
    "atlassian-webresource-webpack-plugin": "^4.9.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "css-loader": "^1.0.1",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "directory-named-webpack-plugin": "^2.2.3",
    "eslint": "^7.21.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "~4.0.0",
    "eslint-webpack-plugin": "^2.5.2",
    "file-loader": "^0.11.2",
    "gulp": "4.0.2",
    "gulp-babel": "^7.0.0",
    "gulp-debug": "4.0.0",
    "gulp-if": "3.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-rename": "1.2.2",
    "gulp-uglify": "^3.0.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom-global": "^2.0.4",
    "jest-serializer-vue": "^0.3.0",
    "js-yaml": "^3.14.0",
    "lazypipe": "^1.0.1",
    "less": "^3.13.1",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^1.3.9",
    "terser-webpack-plugin": "^4.2.3",
    "underscore": "^1.8.3",
    "url-loader": "^4.1.1",
    "vue-jest": "^1.0.2",
    "vue-loader": "^15.9.6",
    "vue-server-renderer": "^2.5.22",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.22",
    "vue-test-utils": "^1.0.0-beta.4",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^3.11.2",
